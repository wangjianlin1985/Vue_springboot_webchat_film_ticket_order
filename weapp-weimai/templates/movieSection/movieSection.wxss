.movie-section {
  position: relative;
  display: flex;
  align-items: center;
  height: 228rpx;
  padding: 0 30rpx;
}
.movie-section::after{
  position: absolute;
  content: '';
  height: 0;
  width: calc(100% - 208rpx);
  right:30rpx;
  bottom:0;
  border-bottom: 1px solid #f0f0f0;
}

.movie-info {
  margin: 0 20rpx;
  font-size: 26rpx;
  color: #666;
  width: 60%;
}

.movie-info>view {
  margin-bottom: 6rpx;
}

.movie-img {
  width: 128rpx;
  height: 180rpx;
}

.movie-name {
  font-size: 34rpx;
  color: #333;
  padding-right: 10rpx;
}

.version{
  display: inline-block;
  height: 28rpx;
  background-size: contain;
  background-repeat: no-repeat;
  margin-left: 10rpx;
  margin-top: 10rpx;
}
.v3d.imax {
  width: 86rpx;
  background-image: url(data:image/png;base64,);
}
.v3d {
  width: 34rpx;
  background-image: url(data:image/png;base64,);
}
.v2d.imax {
  width: 86rpx;
  background-image: url(data:image/png;base64,);
}

.grade {
  font-weight: 700;
  color: #faaf00;
  font-size: 30rpx;
}
